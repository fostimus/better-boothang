<p>Choose your message!</p>

<form action="/user/messages" method="post">
  <ul>
  <% messages.forEach(function(message) { %>
    <li><a href='#' onclick="document.getElementById('message-to-boo').value='<%= message.message %>'"><%= message.message %></a></li>
  <% }) %>
  </ul>

  <input id='message-to-boo' type="text" name="message" value="">
  <button type="submit" name="button">Choose this message</button>
</form>
